([Mitologia_Class30032] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Un personaje no puede tener a la vez el favor y el enojo de un mismo dios")
	(%3APAL-NAME "FavorYEnojoDios")
	(%3APAL-STATEMENT "(forall ?favor \n        (forall ?enojo \n                (=> (and (instance-of ?favor Favor)\n	       (instance-of ?enojo Enojo)  \n                               (own-slot-not-null personaje ?favor) \n                               (own-slot-not-null personaje ?enojo) \n                               (= (dios ?favor) (dios ?enojo))) \n                               (/= (personaje ?favor) (personaje ?enojo))\n                 )\n         )\n)\n(defrange ?personaje :FRAME personaje) \n(defrange ?personaje :FRAME personaje)"))


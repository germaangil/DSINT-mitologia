([Mitologia_Class4] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Para apresar a alguien debe estar libre")
	(%3APAL-NAME "ApresarAAlguienSoloSiEstaLibre")
	(%3APAL-RANGE "(defrange ?apresamiento :FRAME Apresamiento)")
	(%3APAL-STATEMENT "(forall ?apresamiento\n        (=> \n            (and \n                 (instance-of ?apresamiento Apresamiento)\n                 (own-slot-value preso ?apresamiento ?personaje)\n                 (not (own-slot-value estado ?personaje LIBRE))\n            )\n            (false)\n        )\n)"))

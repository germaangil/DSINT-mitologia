([Mitologia_Class2] of  %3APAL-CONSTRAINT

	(%3APAL-DESCRIPTION "Un personaje no puede robarse a si mismo")
	(%3APAL-NAME "NoRobarteATiMismo")
	(%3APAL-STATEMENT "(forall ?robo \n        (=> \n            (and (instance-of ?robo Robo) \n                 (own-slot-not-null personaje ?robo) \n                 (own-slot-not-null personaje2 ?robo))\n            (/= (personaje ?robo) (personaje2 ?robo))\n        )\n)\n(defrange ?personaje :FRAME personaje) \n(defrange ?personaje2 :FRAME personaje2)"))

